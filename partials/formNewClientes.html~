<!-- Custom scripts --> 
<script>

   $("#cep").mask("00000-000");
   $("#cpf").mask("000.000.000-00");
   
  function show() {
	var fields = $( "#form" ).serializeArray();
	var result = "";
	jQuery.each( fields, function( i, field ) {
		result += field.value + " " ;
	});
	alert(result);
  }
  
  $(function () {
   	$('#telTags').tagator({
   		height: '34',
   		width:'110',
   		placeholder:"Digite aqui...",
   		useDimmer: true,	
   		dropdown:[{
			'Celular':'Celular',
			'Residencial':'Residencial',
			'Comercial':'Comercial'
		}]
			
   	});   	
	
  });
</script>
